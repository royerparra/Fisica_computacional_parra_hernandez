   "source": [
    "\n",
    "function pozofini(a,b)\n",
    "    x=b; \n",
    "    f(x)=x*tan(x)-sqrt(a-x*x)\n",
    "    for i in 1:20\n",
    "        x=x-(f(x))/(tan(x)+x/(cos(x)*cos(x))+x/(sqrt(a-x*x)))\n",
    "    end\n",
    "    return x,f(x)\n",
    "end"
   ]
